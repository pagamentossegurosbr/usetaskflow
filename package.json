{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:custom": "node server.js",
    "build": "prisma generate --schema=./prisma/schema.final.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "db:studio": "prisma studio",
    "verify:config": "node scripts/verify-config.js",
    "setup:supabase": "node scripts/setup-supabase.js",
    "clean:supabase": "node scripts/clean-supabase.js",
