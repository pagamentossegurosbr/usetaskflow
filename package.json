    "setup:development": "node scripts/setup-development.js",
    "setup:production": "node scripts/setup-production.js",
    "deploy:production": "node scripts/deploy-production.js",
    "deploy:vercel": "node scripts/deploy-production.js vercel",
    "deploy:netlify": "node scripts/deploy-production.js netlify",
    "deploy:manual": "node scripts/deploy-production.js manual",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "test:build": "npm run build && npm run start",
    "clean": "rm -rf .next out",
    "clean:all": "rm -rf .next out node_modules package-lock.json && npm install"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
