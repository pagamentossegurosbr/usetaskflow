# ========================================
# CONFIGURAÇÃO DE DESENVOLVIMENTO - NOTCH TODO LIST
# ========================================

# ========================================
# SUPABASE CONFIGURATION
# ========================================
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# ========================================
# NEXTAUTH CONFIGURATION
# ========================================
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here_min_32_chars

# ========================================
# STRIPE CONFIGURATION (TESTE)
# ========================================
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Price IDs para teste
NEXT_PUBLIC_STRIPE_PRICE_ASPIRANTE_TRIAL=price_your_aspirante_trial_price_id
NEXT_PUBLIC_STRIPE_PRICE_EXECUTOR_TRIAL=price_your_executor_trial_price_id

# ========================================
# OAUTH PROVIDERS
# ========================================
# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# GitHub OAuth
GITHUB_ID=your_github_id_here
GITHUB_SECRET=your_github_secret_here

# ========================================
# DATABASE CONFIGURATION
# ========================================
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# ========================================
# MONITORING & ANALYTICS (OPCIONAL)
# ========================================
# Vercel Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_vercel_analytics_id

# Sentry
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# ========================================
# FEATURE FLAGS
# ========================================
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_MONITORING=false
NEXT_PUBLIC_ENABLE_BETA_FEATURES=true

# ========================================
# SECURITY
# ========================================
# Rate limiting
RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_WINDOW_MS=900000

# CORS
NEXT_PUBLIC_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# ========================================
# PERFORMANCE
# ========================================
# Cache settings
NEXT_PUBLIC_CACHE_TTL=3600
NEXT_PUBLIC_API_TIMEOUT=10000

# ========================================
# EMAIL CONFIGURATION (OPCIONAL)
# ========================================
# Resend
RESEND_API_KEY=your_resend_api_key_here
NEXT_PUBLIC_FROM_EMAIL=noreply@localhost.com

# ========================================
# STORAGE (OPCIONAL)
# ========================================
# Supabase Storage
NEXT_PUBLIC_SUPABASE_STORAGE_URL=https://your-project.supabase.co/storage/v1

# ========================================
# ENVIRONMENT
# ========================================
NODE_ENV=development
NEXT_PUBLIC_APP_ENV=development
